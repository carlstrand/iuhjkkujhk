    <script>
        require([
            "jquery",
            "flexsliderJS",
            "froogaLoop",
            "flexsliderLazyJS",
            "flexsliderLazyBgJS",
            "flexsliderLazyVideoJS"
        ], function ($) {
            function slideAction(startat,auto){
                var src = $('#clever-left-slide-'+startat+'-slideshow').find('iframe').attr('src');
                if(src.indexOf("autoplay=0") > 0) {
                    $('#clever-left-slide-'+startat+'-slideshow').find('iframe').attr('src',src.replace('autoplay=0','autoplay=1')) ;
                } else if(src.indexOf("autoplay=1") > 0){

                } else {
                    $('#clever-left-slide-'+startat+'-slideshow').find('iframe').attr('src',src+'&autoplay=1');
                }
                if(auto == '1' || auto == 1) {
                    $('#clever-left-slide-'+startat+'-slideshow #play-'+startat).hide();
                    $('#clever-left-slide-'+startat+'-slideshow #pause-'+startat).show();
                }

                if (auto == '2' || auto == 2) {
                    $('#clever-left-slide-'+startat+'-slideshow').find('iframe').css('pointer-events','none');
                }
            }
            function viewLayout($layout,flexslider) {
                var $left =  $('#<?php  echo $uniqueS;  ?>').offset().left;
                var $top =  $('#<?php  echo $uniqueS;  ?>').offset().top;
                var $window_h = $(window).height();
                if($layout == 0 || $layout == '0' || $layout == 1 || $layout == '1' ) {
                    $('#<?php  echo $uniqueS;  ?>').css({
                        'position':'relative',
                        'width':$(window).width()+'px'
                    });
                    <?php if($rtl_language) : ?>
                    $('#<?php  echo $uniqueS;  ?>').css({'left':'0','right':'-'+$left+'px' });
                    <?php else : ?>
                    $('#<?php  echo $uniqueS;  ?>').css({'right':'0','left':'-'+$left+'px' });
                    <?php endif; ?>
                }
                $('#<?php  echo $uniqueS;  ?> .flex-viewport').css('width','100%');
                //give height
                if($layout == 0 || $layout == '0' || $layout == 2 || $layout == '2' ) {
                    <?php
                    $height = $slider->getData('slide_height');
                    if(empty($height)) $height = '600';
                     if (!empty($height)) {
                     ?>
                    var $height = '<?php echo $height ?>';
                    $('#<?php  echo $uniqueS;  ?>').height($height);
                    <?php } ?>
                } else {
                    var $height = $window_h - $top;
                    $('#<?php  echo $uniqueS;  ?>').height($height);
                }
                if ($height) {$('#<?php  echo $uniqueS;  ?>').find('img,iframe').css({'width':'100%','height':$height});}
                window.size = 1;
                if(!$_single || $_single == '0') {
                    var $_size = getGridSize(<?php echo $slider->getData('number_item_480'); ?>,<?php echo $slider->getData('number_item_768'); ?>,<?php echo $slider->getData('number_item_992'); ?>,<?php echo $slider->getData('number_item_1200'); ?>);
                    if ($_size > 0) {
                        window.size = $_size;
                        if(flexslider) {
                            flexslider.vars.minItems = $_size;
                            flexslider.vars.maxItems = $_size;
                        }
                    }
                }
            }

            function getGridSize($48,$768,$992,$1200) {
                return (window.innerWidth <= 480) ? 1 : ((window.innerWidth > 480 && window.innerWidth <= 768) ? $48 : ((window.innerWidth > 768 && window.innerWidth <= 992) ? $768 : ((window.innerWidth > 992 && window.innerWidth <= 1200) ? $992 : $1200) ));
            }

            function initVideo(slider){
                var slide = slider.currentSlide;
                var _from = slide * slider.visible;
                var _to = _from + (slider.visible-1);
                for (var i = _from; i <= _to; i++) {
                    if($('#clever-left-slide-'+i+'-slideshow').find('iframe').length > 0) {
                        var auto = $('#clever-left-slide-'+i).val();
                        if (auto ==1 || auto == '1' || auto ==2 || auto == '2') {
                            slideAction(i,auto);
                        }
                    }
                    if(!window._start) {
                        $('#clever-left-slide-'+i+'-slideshow').lazyLoadXT({show: true});
                    }
                    $('#clever-left-slide-'+i+'-slideshow').addClass('flex-active-slide');
                }
                $(document).on('lazyshow', function (e) {
                    $(e.target).parent('li').removeClass('clever-slider-lazyloader');
                });
                window._start = true;
            }

            function doSliderFlex() {
                var $_i_m = (size > 1) ? 1 :  0;
                var $_i_w = ($('#<?php  echo $uniqueS;  ?>').width())/size - $_i_m;
                $('#<?php  echo $uniqueS;  ?>').flexslider({
                    animation: "slide",
                    controlNav: <?php echo  $slider->getData('show_pagination') ? 'true' : 'false' ?>,
                    directionNav: <?php echo  $slider->getData('show_nav') ? 'true' : 'false' ?>,
                    slideshow: <?php echo  $slider->getData('auto_rotate')  ? 'true' : 'false'  ?>,
                    itemWidth:$_i_w,
                    itemMargin:$_i_m,
                    minItems: size,
                    maxItems: size,
                    slideshowSpeed:<?php echo  $slider->getData('slide_animation') ; ?>,
                    start: function(slider){
                        window._start = false;
                        if(!window.initVideo){
                            initVideo(slider);
                            window.initVideo = true;
                        }
                        flexslider = slider;
                    },
                    init: function(slider) {
                        window.initVideo = false;
                    },
                    before:function(slider){
                        window.action_slide = false;
                    },
                    after: function(slider) {
//                        $(window).scrollTop($(window).scrollTop()+1);
                        if(!window.action_slide){
                            initVideo(slider);
                            if (layout == 0 || layout == '0' ||  layout == 2 || layout == '2' ) {
                                $('#<?php  echo $uniqueS;  ?>').find('img,iframe').css({'height':$('#<?php  echo $uniqueS;  ?> ul.slides').height()});
                            }
                            window.action_slide = true;
                        }
                    }
                });
            }
            $(function () {
                //
                window.$_single = <?php echo $slider->getData('single_slide'); ?>;
                window.flexslider = { vars:{} };
                var $layout = <?php echo $layout ?>;
                window.layout = $layout;
                viewLayout(layout,false);
                $(window).on('resize',function(){
                    viewLayout(layout,flexslider);
                });
                //
                doSliderFlex();

//                if ($('#clever-left-slide-0').length > 0) {
//                    var auto_0 = $('#clever-left-slide-0').val();
//                    if (auto ==1 || auto == '1' || auto ==2 || auto == '2') {
//                        slideAction(0, auto_0);
//                    }
//                }
                <?php if (count($youtube_list) > 0) {?>
                <?php foreach($youtube_list as $yl) { ?>
                var player<?php echo $yl; ?>;
                <?php } ?>
                window.onYouTubePlayerAPIReady = function() {
                    <?php foreach($youtube_list as $key=>$ys) { ?>
                    player<?php echo $ys; ?> = new YT.Player('<?php echo $youtube_unique[$key]; ?>', {
                        events: {
                            'onReady': onPlayerReady<?php echo $ys; ?>
                        }
                    });
                    <?php } ?>
                }
                <?php foreach($youtube_list as $k=>$yt) { ?>
                function onPlayerReady<?php echo $yt; ?>(event) {
                    var $style = <?php echo $y_content_style[$k] ?>;
                    if ($style != '2' || $style != 2) {
                        var playButton<?php echo $yt; ?> = document.getElementById("play-<?php echo $yt; ?>");
                        var pauseButton<?php echo $yt; ?> = document.getElementById("pause-<?php echo $yt ?>");
                        playButton<?php echo $yt; ?>.addEventListener("click", function () {
                            player<?php echo $yt; ?>.playVideo();
                            document.getElementById("image-<?php echo $youtube_unique[$k] ?>").style.display = 'none';
                            document.getElementById("<?php echo $youtube_unique[$k] ?>").removeAttribute('style');
                            playButton<?php echo $yt; ?>.style.display = 'none';
                            pauseButton<?php echo $yt; ?>.style.display = 'inline-block';
                        });
                        pauseButton<?php echo $yt; ?>.addEventListener("click", function () {
                            player<?php echo $yt; ?>.pauseVideo();
                            playButton<?php echo $yt; ?>.style.display = 'inline';
                            pauseButton<?php echo $yt; ?>.style.display = 'none';
                        });
                    }
                }
                <?php } ?>
                <?php }  ?>
                <?php if (count($vimeo_list) > 0) {?>
                <?php foreach($vimeo_list as $t=>$vl) { ?>
                var v_style = <?php echo $v_content_style[$t] ?>;
                var iframe<?php echo $vl; ?> = document.getElementById('<?php echo $vimeo_unique[$t]; ?>');
                // $f == Froogaloop
                var player<?php echo $vl; ?> = $f(iframe<?php echo $vl; ?>);
                if(v_style != '2' || v_style != 2) {
                    // bind events
                    var playButton<?php echo $vl; ?> = document.getElementById("play-<?php echo $vl; ?>");
                    var pauseButton<?php echo $vl; ?> = document.getElementById("pause-<?php echo $vl; ?>");

                    playButton<?php echo $vl; ?>.addEventListener("click", function () {
                        player<?php echo $vl; ?>.api("play");
                        document.getElementById("image-<?php echo $vimeo_unique[$t] ?>").style.display = 'none';
                        document.getElementById("<?php echo $vimeo_unique[$t] ?>").removeAttribute('style');
                        playButton<?php echo $vl; ?>.style.display = 'none';
                        pauseButton<?php echo $vl; ?>.style.display = 'inline';
                    });

                    pauseButton<?php echo $vl; ?>.addEventListener("click", function () {
                        player<?php echo $vl; ?>.api("pause");
                        playButton<?php echo $vl; ?>.style.display = 'inline';
                        pauseButton<?php echo $vl; ?>.style.display = 'none';
                    });
                } else {
                    player<?php echo $vl; ?>.api('setVolume', 0);
                }
                <?php } ?>
                <?php }  ?>
                <?php if (count($youtube_list) > 0) {?>
                var tag = document.createElement('script');
                tag.src = "//www.youtube.com/player_api";
                var firstScriptTag = document.getElementsByTagName('script')[0];
                firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
                <?php } ?>
            })
        });
    </script>
<?php endif ?>